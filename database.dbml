// Use DBML to define your database structure// Docs: https://dbml.dbdiagram.io/docsTable PERMISSIONS {  IDT_PERMISSION integer [primary key]  NAME varchar [not null]}Table BLOG_USER {  IDT_BLOG_USER integer [primary key]  IDT_PERMISSION integer [not null]  USERNAME varchar [not null]  EMAIL varchar [not null]  PASSWORD varchar [not null]  FLG_ACTIVE bit [not null]}Table BLOG_POST {  IDT_BLOG_POST integer [primary key]  IDT_BLOG_USER integer [not null]  TITLE varchar [not null]  SUBTITLE varchar [not null]  CONTENT text [null]  DATE_PUBLISH timestamp [not null]  FLG_DELETED bit [not null]}Table KEYWORD {  IDT_KEYWORD integer [primary key]  NAME varchar [not null]  CATEGORY varchar [not null]}Table BLOG_POST_KEYWORD {  IDT_POST_KEYWORD integer [primary key]  IDT_BLOG_POST integer [not null]    IDT_KEYWORD integer [not null]}Table BLOG_COMMENT {  IDT_BLOG_COMMENT integer [primary key]  IDT_BLOG_USER integer [not null]  IDT_PARENT_BLOG_COMMENT integer [null]  IDT_BLOG_POST integer [not null]  CONTENT text  DATE_PUBLISH timestamp [not null]  FLG_DELETED bit [not null]}Ref: BLOG_USER.IDT_PERMISSION > PERMISSIONS.IDT_PERMISSIONRef: BLOG_POST.IDT_BLOG_USER > BLOG_USER.IDT_BLOG_USERRef: BLOG_POST_KEYWORD.IDT_KEYWORD > KEYWORD.IDT_KEYWORDRef: BLOG_POST_KEYWORD.IDT_BLOG_POST > BLOG_POST.IDT_BLOG_POSTRef: BLOG_COMMENT.IDT_BLOG_USER > BLOG_USER.IDT_BLOG_USERRef: BLOG_COMMENT.IDT_BLOG_POST > BLOG_POST.IDT_BLOG_POST